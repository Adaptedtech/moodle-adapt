# Cron files
files:
    "/etc/cron.d/moodle-cron":
        mode: "000644"
        owner: root
        group: root
        content: |
            * * * * *  root . /opt/elasticbeanstalk/deployment/env && /usr/bin/php /var/www/html/admin/cli/cron.php > /dev/null
 
commands:
    remove_old_cron:
        command: "rm -f /etc/cron.d/*.bak"